<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/AL.png"
            level-id="activelearning"
            locked="false"
            name="Active Learning"
            path="/labs/">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="score"
            locked="false"
            name="Score"
            path="/labs/activelearning/">
      </category>
            <category
            icon="icons/missing.gif"
            level-id="select"
            locked="false"
            name="Select"
            path="/labs/activelearning/">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="density"
            locked="false"
            name="Density"
            path="/labs/activelearning/score">
      </category>
	  <category
            icon="icons/missing.gif"
            level-id="uncertainty"
            locked="false"
            name="Uncertainty"
            path="/labs/activelearning/score">
      </category>
       <category
            icon="icons/missing.gif"
            level-id="novelty"
            locked="false"
            name="Novelty Detection"
            path="/labs/activelearning/score">
      </category>
	  <category
            icon="icons/al_loop_category.png"
            level-id="loop"
            locked="false"
            name="Loop"
            path="/labs/activelearning">
      </category>
	  <category
            icon="icons/missing.gif"
            level-id="combiner"
            locked="false"
            name="Combiner"
            path="/labs/activelearning/score">
      </category>
	  <category
            icon="icons/missing.gif"
            level-id="allinone"
            locked="false"
            name="All in One"
            path="/labs/activelearning/score">
      </category>
   <category
         icon="icons/missing.gif"
         level-id="legacy"
         locked="false"
         name="Legacy"
         path="/labs/activelearning">
   </category>
   </extension>

   <!-- Nodesets -->
   <extension
         point="org.knime.workbench.repository.nodesets">
         <nodeset
               deprecated="true"
               factory-class="org.knime.al.nodes.score.density.DensityNodeSetFactory">
      </nodeset>
   		<nodeset
           deprecated="true"
           factory-class="org.knime.al.nodes.loop.LoopNodeSetFactory">
        </nodeset>
        <nodeset
              deprecated="false"
              factory-class="org.knime.al.nodes.score.novelty.NoveltyNodeSetFactory">
        </nodeset>
        <nodeset
              deprecated="false"
              factory-class="org.knime.al.nodes.loop.ActiveLearningLoopNodeSetFactory">
        </nodeset>
       <!-- <nodeset
            factory-class="org.knime.al.nodes.score.novelty.NoveltyNodeSetFactory">
        </nodeset> -->
     </extension>
     
     <!-- Nodes -->
     <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/labs/activelearning/score/allinone"
            deprecated="true"
            factory-class="org.knime.al.nodes.score.pbac.PBACScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/select"
            deprecated="true"
            factory-class="org.knime.al.nodes.select.elementselector.ElementSelectorNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/uncertainty"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.uncertainty.entropy.EntropyScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/uncertainty"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.uncertainty.margin.MarginScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/uncertainty"
            deprecated="true"
            factory-class="org.knime.al.nodes.score.uncertainty.variance.VarianceScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/uncertainty"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.uncertainty.variance.VarianceScorer2NodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/uncertainty"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.uncertainty.leastconfident.LeastConfidentScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/combiner"
            deprecated="true"
            factory-class="org.knime.al.nodes.score.combine.pbac.UncertaintyDistributionScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/combiner"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.combine.pbac.UncertaintyDistributionScorer2NodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/density"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.density.nodepotential.PotentialInitializerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/density"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.density.scorer.DensityScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/density"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.density.graphdensity.GraphDensityInitializerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/score/density"
            deprecated="false"
            factory-class="org.knime.al.nodes.score.density.updater.DensityUpdaterNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/"
            deprecated="false"
            factory-class="org.knime.al.nodes.labelingWidget.ActiveLabelingNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/legacy"
            deprecated="false"
            factory-class="org.knime.al.nodes.legacy.score.density.graphdensity.GraphDensityScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/legacy"
            deprecated="false"
            factory-class="org.knime.al.nodes.legacy.score.density.nodepotential.NodePotentialScorerNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/legacy"
            deprecated="false"
            factory-class="org.knime.al.nodes.legacy.loop.dbg.DBGActiveLearnLoopEndNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/legacy"
            deprecated="false"
            factory-class="org.knime.al.nodes.legacy.loop.start.ActiveLearnLoopStartNodeFactory">
      </node>
      <node
            category-path="/labs/activelearning/legacy"
            deprecated="false"
            factory-class="org.knime.al.nodes.legacy.loop.end.ActiveLearnLoopEndNodeFactory">
      </node>
   </extension>
     <extension
           point="org.knime.core.PortType">
        <portType
              hidden="false"
              name="KNFST Model"
              objectClass="org.knime.al.nodes.score.novelty.knfstlearner.KNFSTPortObject"
              objectSerializer="org.knime.al.nodes.score.novelty.knfstlearner.KNFSTPortObject$Serializer"
              specClass="org.knime.al.nodes.score.novelty.knfstlearner.KNFSTPortObjectSpec"
              specSerializer="org.knime.al.nodes.score.novelty.knfstlearner.KNFSTPortObjectSpec$Serializer">
        </portType>
        <portType
              hidden="false"
              name="Density Scorer Model"
              objectClass="org.knime.al.nodes.score.density.DensityScorerPortObject"
              objectSerializer="org.knime.al.nodes.score.density.DensityScorerPortObject$Serializer"
              specClass="org.knime.al.nodes.score.density.DensityScorerPortObjectSpec"
              specSerializer="org.knime.al.nodes.score.density.DensityScorerPortObjectSpec$Serializer">
        </portType>
     </extension>
     <extension
           point="org.knime.core.NodeFactoryClassMapper">
        <NodeFactoryClassMapper
              classMapper="org.knime.al.nodes.score.combine.pbac.UncertaintyDistributionScorerNodeFactoryMapper">
        </NodeFactoryClassMapper>
     </extension>
     <extension
           point="org.knime.js.core.javascriptComponents">
        <javascriptComponent
              getComponentValue-method-name="getComponentValue"
              implementationBundleID="org.knime.activelearning.labelingWidget.bundle"
              init-method-name="init"
              javascriptComponentID="org.knime.activelearning.labelingWidget"
              name="Active Labeling Widget"
              namespace="generalPurposeLabelingWidget"
              setValidationError-method-name="setValidationError"
              validate-method-name="validate">
        </javascriptComponent>
     </extension>
     <extension
           point="org.knime.js.core.webResources">
        <webResourceBundle
              debug="true"
              name="Active Learning JS"
              usesDefine="true"
              version="1.0.0"
              webResourceBundleID="org.knime.activelearning.labelingWidget.bundle">
           <webResource
                 relativePathSource="js-src/org/knime/activelearning/"
                 relativePathTarget="/org/knime/activelearning/">
           </webResource>
           <importResource
                 relativePath="/org/knime/activelearning/labelingWidget.js"
                 type="JAVASCRIPT">
           </importResource>
           <importResource
                 relativePath="/org/knime/activelearning/labelingWidget.css"
                 type="CSS">
           </importResource>
           <webDependency
                 webResourceBundleID="KnimeTableView_1.0.0">
           </webDependency>
           <webDependency
                 webResourceBundleID="KnimeTable_1.0.0">
           </webDependency>
           <webDependency
                 webResourceBundleID="org.knime.js.base.node.viz.pagedTable.bundle">
           </webDependency>
           <webDependency
                 webResourceBundleID="org.knime.js.base.node.viz.tileView.bundle">
           </webDependency>
           <webDependency
                 webResourceBundleID="knimeService_1.0">
           </webDependency>
        </webResourceBundle>
     </extension>
</plugin>
